<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
            http://www.springframework.org/schema/beans 
            http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="emailSender" class="com.yeahmobi.yscheduler.notice.DefaultEmailSender">
		<property name="mailApi">
			<value>${notice.emailApi}</value>
		</property>
	</bean>

	<bean id="smsSender" class="com.yeahmobi.yscheduler.notice.DefaultSmsSender">
		<property name="smsApi">
			<value>${notice.smsApi}</value>
		</property>
		<property name="app">
			<value>${notice.smsApp}</value>
		</property>
		<property name="appKey">
			<value>${notice.smsAppKey}</value>
		</property>
	</bean>


	<bean id="contentHelper" class="com.yeahmobi.yscheduler.notice.ContentHelper">
		<property name="workflowUrl">
			<value>${notice.workflowInstanceUrl}</value>
		</property>
		<property name="taskUrl">
			<value>${notice.taskInstanceUrl}</value>
		</property>
	</bean>




</beans>